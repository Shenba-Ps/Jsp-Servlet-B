$(function() {

});

document.addEventListener("DOMContentLoaded", function() {
	OpenCustomerDetail();
});

function OpenCustomerDetail() {
	$('#mdlCustomerDetail').modal('show');
}

function onclickBookSlot() {
	$('#idName').removeClass("ControlError");
	if ($('#idName').val() == "") {
		$('#idName').addClass("ControlError");
		$('#errName').innerHTML = "";
	}
}